{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "CookieSettings": {
    "Audience": "VoroLp.API_Audience",
    "Issuer": "VoroLp.API_Issuer",
    "Name": "VoroLp",
    "AccessDeniedPath": "/Unauthorized",
    "LoginPath": "/SignIn",
    "Path": "/",
    "ExpireHours": 12
  },
  "CorsSettings": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "http://localhost:4200",
      "https://localhost:3000",
      "https://localhost:4200",
      "https://vorolabs.app"
    ]
  }
}
